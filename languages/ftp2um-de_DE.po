#: index.php:380
#@ index
msgid "FTP Host&Port"
msgstr "FTP Hostname & Port"

#: index.php:382
#@ index
msgid "Please enter your ftp hostname and port. E.g. <code>mydomain.com:21</code>"
msgstr "Bitte geben Sie ihren FTP-Hostnamen und den FTP-Port ein. Zum Beispiel: <code>meine-domain.de:21</code>. Wenn kein Port angegeben wird, wird der Standard-Port 21 verwendet."

#: index.php:385
#@ index
msgid "FTP Username"
msgstr "FTP Benutzername"

#: index.php:387
#@ index
msgid "Please enter your ftp username"
msgstr "Bitte geben Sie ihren FTP-Benutzernamen ein"

#: index.php:390
#@ index
msgid "FTP Password"
msgstr "FTP Passwort"

#: index.php:392
#@ index
msgid "Please enter your ftp password"
msgstr "Bitte geben Sie ihr FTP-Passwort ein"

